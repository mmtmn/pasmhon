class Person:
    def __init__(self, name: str, age: int):
        self.name = name
        self.age  = age

    def birthday(self):
        self.age = self.age + 1

    def greet(self):
        print("hi, I am " + self.name)

p = Person("alice", 30)
p.greet()
p.birthday()
print(p.age)

